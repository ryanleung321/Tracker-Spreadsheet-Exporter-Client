{
  "name": "AutoQA tab thing",
  "version": "0.1",
  "manifest_version": 2,  
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html", 
    "default_title": "click me"
  },

    "background":
  {
    "script": "background.js"
  },

  "permissions": [
    "tabs", 
    "alarms", 
    "activeTab", 
    "https://ajax.googleapis.com/"
  ]

}
